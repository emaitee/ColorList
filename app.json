{
  "expo": {
    "name": "Color List",
    "slug": "color-list",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/color-list.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.issatoyin.colorList"
    },
    "android": {
      "package": "com.issatoyin.colorList"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "description": "A react-native project used to implement the basic components of RN",
    "githubUrl": "https://github.com/emaitee/color-list"
  }
}